 * Licensed under MIT (https://github.com/gabyliu/performance-report/blob/master/LICENSE)
 */const u8array={stringify:function(e){const t=e.words,i=e.sigBytes,n=new Uint8Array(i);for(let a=0;a<i;a++){const e=t[a>>>2]>>>24-a%4*8&255;n[a]=e}return n},parse:function(e){const t=e.length,i=[];for(let n=0;n<t;n++)i[n>>>2]|=(255&e[n])<<24-n%4*8;return cryptoJsExports.lib.WordArray.create(i,t)}};var CryptoU8array={u8array:u8array};function hexToBytes(e){const t=[];for(let i=0;i<e.length;i+=2)t.push(parseInt(e.substr(i,2),16));return t}function judgeType(e){switch(e.slice(0,2)){case"ff":return"image/jpg";case"89":return"image/png";case"47":return"image/gif"}return""}function atob$1(e){const t=new Uint8Array(256);for(let o=0;o<64;o++)t["ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/".charCodeAt(o)]=o;let i="",n=0,a=0;for(let o=0;o<e.length;o++){const r=e.charCodeAt(o);61!==r&&(n=n<<6|t[r],a++,4===a&&(i+=String.fromCharCode(n>>16&255),i+=String.fromCharCode(n>>8&255),i+=String.fromCharCode(255&n),a=0,n=0))}return 2===a?i+=String.fromCharCode(n>>4&255):3===a&&(i+=String.fromCharCode(n>>10&255),i+=String.fromCharCode(n>>2&255)),i}function Base64toArrayBuffer(e){const t=e.replace(/[^A-Za-z0-9+/=-]/g,""),i=(4-t.length%4)%4,n=atob$1(t.padEnd(t.length+i,"=").replace(/-/g,"+").replace(/_/g,"/")),a=new Uint8Array(n.length);for(let o=0;o<n.length;o++)a[o]=n.charCodeAt(o);return a}function decrypt({buffer:e,key:t,iv:i}){const n=hexToBytes(t),a=hexToBytes(i),o=new Uint8Array(n),r=new Uint8Array(a),s=CryptoU8array.u8array.parse(o),l=CryptoU8array.u8array.parse(r),U=new Uint8Array(e),c=CryptoU8array.u8array.parse(U).toString(cryptoJsExports.enc.Base64),u=cryptoJsExports.AES.decrypt(c,s,{iv:l,mode:cryptoJsExports.mode.CBC,padding:cryptoJsExports.pad.Pkcs7}),d=u.toString(cryptoJsExports.enc.Hex),A=u.toString(cryptoJsExports.enc.Base64),m="data:".concat(judgeType(d),";base64,").concat(A);let p=Base64toArrayBuffer("");try{p=Base64toArrayBuffer(A)}catch(e3){console.log("decrypt err: ",e3)}return{base64:A,dataURL:m,fileBuffer:p}}function fileDecode({url:e,buffer:t,a